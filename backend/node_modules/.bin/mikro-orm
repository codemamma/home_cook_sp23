#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/d/workspace/home_cook_sp23/backend/node_modules/.pnpm/@mikro-orm+cli@5.6.16_@mikro-orm+postgresql@5.6.16_@mikro-orm+seeder@5.6.16_pg@8.10.0/node_modules/@mikro-orm/cli/node_modules:/home/d/workspace/home_cook_sp23/backend/node_modules/.pnpm/@mikro-orm+cli@5.6.16_@mikro-orm+postgresql@5.6.16_@mikro-orm+seeder@5.6.16_pg@8.10.0/node_modules/@mikro-orm/node_modules:/home/d/workspace/home_cook_sp23/backend/node_modules/.pnpm/@mikro-orm+cli@5.6.16_@mikro-orm+postgresql@5.6.16_@mikro-orm+seeder@5.6.16_pg@8.10.0/node_modules:/home/d/workspace/home_cook_sp23/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/d/workspace/home_cook_sp23/backend/node_modules/.pnpm/@mikro-orm+cli@5.6.16_@mikro-orm+postgresql@5.6.16_@mikro-orm+seeder@5.6.16_pg@8.10.0/node_modules/@mikro-orm/cli/node_modules:/home/d/workspace/home_cook_sp23/backend/node_modules/.pnpm/@mikro-orm+cli@5.6.16_@mikro-orm+postgresql@5.6.16_@mikro-orm+seeder@5.6.16_pg@8.10.0/node_modules/@mikro-orm/node_modules:/home/d/workspace/home_cook_sp23/backend/node_modules/.pnpm/@mikro-orm+cli@5.6.16_@mikro-orm+postgresql@5.6.16_@mikro-orm+seeder@5.6.16_pg@8.10.0/node_modules:/home/d/workspace/home_cook_sp23/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mikro-orm/cli/cli.js" "$@"
else
  exec node  "$basedir/../@mikro-orm/cli/cli.js" "$@"
fi
